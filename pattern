import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        int i=1;


        while (i<=n){

            int j =1;
            while (j<=n){
                char ch=(char)('A'+i-1);
                 System.out.print(ch);

                j=j+1;

            }
            System.out.println();
            i=i+1;

        }
    }
}
OUTPUT
3
AAA
BBB
CCC
##12 code
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        int i=1;


        while (i<=n){

            int j =1;
            while (j<=n){
                char ch=(char)('A'+i+j-2);
                 System.out.print(ch);

                j=j+1;

            }
            System.out.println();
            i=i+1;

        }
    }
}

OUTPUT
5
ABCDE
BCDEF
CDEFG
DEFGH
EFGHI
##13
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        int i=1;


        while (i<=n){

            int j =1;
            while (j<=i){
                char ch=(char)('A'+i+j-2);

                 System.out.print(ch);
            
                j=j+1;

            }
            System.out.println();
            i=i+1;

        }
    }
}
OUTPUT
2
A
BC
## 14
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
       int i=1;
       while(i<=n){
           int space = n-i;
           while( space>0){
               System.out.print(" ");
               space =space-1;
           }
           int j=1;
           while(j<=i){
               System.out.print("*");
               j=j+1;

           }
           System.out.println();
           i=i+1;
       }
    }
}
OUTPUT
4
   *
  **
 ***
****
##15
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
       int i=1;
       while(i<n){
           int space = i;
           while (space > 0) {
               System.out.print(" ");
               space--;
           }


               int star = n-i+1;
               while( star>0){
                   System.out.print("*");
                   star--;
               }



           System.out.println();
           i=i+1;
       }
    }
}
output
4
 ****
  ***
   **








