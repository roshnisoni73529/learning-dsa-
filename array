cpratice code 1
class Main {
    public static void main(String[] args) {
        int arr[] = {3, 2, 1, 5, 2, 1};  // array initialization
        int largest = arr[0];            // assume first element is lAargest

        for (int i = 0; i < arr.length; i++) {
            if (arr[i] > largest) {
                largest = arr[i];        // update largest
            }
        }

        System.out.println("Largest = " + largest);
    }
}
